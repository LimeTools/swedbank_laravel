{
    "name": "limetools/swedbank_laravel",
    "description": "Laravel package for Swedbank Payment Initiation API V3 integration",
    "keywords": [
        "swedbank",
        "payment",
        "laravel",
        "payment-initiation",
        "api",
        "v3"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/LimeTools/swedbank_laravel",
    "support": {
        "issues": "https://github.com/LimeTools/swedbank_laravel/issues",
        "source": "https://github.com/LimeTools/swedbank_laravel"
    },
    "authors": [
        {
            "name": "Swedbank Laravel Package Contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "LimeTools\\Swedbank\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LimeTools\\Swedbank\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LimeTools\\Swedbank\\SwedbankPaymentServiceProvider"
            ],
            "aliases": {
                "SwedbankPayment": "LimeTools\\Swedbank\\Facades\\SwedbankPayment"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
